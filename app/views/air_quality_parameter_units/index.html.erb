<p id="notice"><%= notice %></p>

<h1>Air Quality Parameter Units</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Symbol</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @air_quality_parameter_units.each do |air_quality_parameter_unit| %>
      <tr>
        <td><%= air_quality_parameter_unit.name %></td>
        <td><%= air_quality_parameter_unit.symbol %></td>
        <td><%= link_to 'Show', air_quality_parameter_unit %></td>
        <td><%= link_to 'Edit', edit_air_quality_parameter_unit_path(air_quality_parameter_unit) %></td>
        <td><%= link_to 'Destroy', air_quality_parameter_unit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Air Quality Parameter Unit', new_air_quality_parameter_unit_path %>
