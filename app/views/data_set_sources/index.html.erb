<p id="notice"><%= notice %></p>

<h1>Data Set Sources</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Confidence level</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_set_sources.each do |data_set_source| %>
      <tr>
        <td><%= data_set_source.name %></td>
        <td><%= data_set_source.confidence_level %></td>
        <td><%= link_to 'Show', data_set_source %></td>
        <td><%= link_to 'Edit', edit_data_set_source_path(data_set_source) %></td>
        <td><%= link_to 'Destroy', data_set_source, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Set Source', new_data_set_source_path %>
